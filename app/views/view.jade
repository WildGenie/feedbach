#view
  .header.span12
    h4 {{survey.id}}
      small {{survey.description}}
    button.btn.btn-warning(ng-click=('resetFeedback(survey.id)')) Reset feedback
    button.btn.btn-danger(ng-click=('deleteSurvey(survey.id)')) Delete
  .span6.questions(ng-repeat='q in survey.questions')
    span.question {{q.question}}
    .answers(ng-repeat='a in q.answers')
      .span4.answer {{a.answer}}
      .span8: progress(percent='a.percent', auto-text='true')
  .bottom: strong Votes: {{survey.totalVotes}}
not-found-modal
